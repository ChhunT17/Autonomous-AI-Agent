# Twitter API OAuth1 Credentials
TWITTER_API_KEY=your_api_key_here
TWITTER_API_SECRET=your_api_secret_here
TWITTER_ACCESS_TOKEN=your_access_token_here
TWITTER_ACCESS_TOKEN_SECRET=your_access_token_secret_here
TWITTER_BEARER_TOKEN=your_bearer_token_here

# Twitter OAuth 2.0 Credentials (for posting)
TWITTER_CLIENT_ID=your_twitter_client_id_here
TWITTER_CLIENT_SECRET=your_twitter_client_secret_here

# OpenRouter API Credentials
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenRouter Model Configuration
# You can use any model available on OpenRouter
# Examples: anthropic/claude-3.5-sonnet, openai/gpt-4-turbo, google/gemini-pro, meta-llama/llama-3-70b-instruct
# Free models: deepseek/deepseek-r1-0528-qwen3-8b:free, nousresearch/hermes-3-llama-3.1-405b:free
# Check https://openrouter.ai/models for all available models
OPENROUTER_MODEL=your_preferred_model_here

# Wallet Configuration
WALLET_PRIVATE_KEY=your_wallet_private_key_here
ETH_RPC_URL=your_ethereum_rpc_url_here
ETH_NETWORK=mainnet
